{
    "name": "cijic/phpmorphy",
    "description": "phpMorphy - morphological analyzer library for Russisan, English languages.",
    "keywords": [
        "phpMorphy"
    ],
    "homepage": "https://github.com/cijic/phpmorphy",
    "license": "MIT",
    "authors": [
        {
            "name": "Chiril Teterea",
            "email": "altcode@ya.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
    },
    "autoload": {
        "psr-4": {
            "Cijic\\PHPMorphy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cijic\\PHPMorphy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
